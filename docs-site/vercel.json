{
    "buildCommand": "cd docs-site && npm run build",
    "outputDirectory": "docs-site/.vitepress/dist",
    "framework": null,
    "installCommand": "cd docs-site && npm install",
    "devCommand": "cd docs-site && npm run dev",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/docs/:path*",
            "destination": "/:path*",
            "permanent": true
        }
    ]
}